#!/usr/bin/env python

from lapchart_gui import LapChartGUI

my_version = '0.1'

if __name__ == '__main__':
    gui = LapChartGUI()
    gui.master.title('ChartRoom v%s' % my_version)

    gui.getCell(1,1).set("10")
    gui.getCell(1,2).set("2")
    gui.getCell(1,3).set("08")
    gui.getCell(1,4).set("55")
    gui.getCell(1,8).set("42")

    gui.getCell(2,1).set("10")
    gui.getCell(2,2).set("08")
    gui.getCell(2,3).set("2")
    gui.getCell(2,4).set("55")

    gui.getCell(4,1).set("08")
    gui.getCell(4,2).set("10")
    gui.getCell(4,3).set("2")
    gui.getCell(4,4).set("55")

    gui.getCell(1,15).set("15")
    gui.getCell(14,1).set("1")

    gui.mainloop()
